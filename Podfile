  pod 'R.swift'

  # DI
  pod 'Swinject', '~> 2.5.0'

  # Network
  pod 'Moya'
  pod 'Firebase/Analytics'

  #UI
  pod 'SnapKit'
  pod 'Kingfisher/SwiftUI'
  pod 'SkeletonUI'

  target 'RKTests' do
    inherit! :search_paths
    # Pods for testing
  end

end
