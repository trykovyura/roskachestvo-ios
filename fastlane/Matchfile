git_url("git@github.com:trykovyura/roskachestvo-ios-certificates.git")

storage_mode("git")

type("development") # The default type, can be: appstore, adhoc, enterprise or development

app_identifier("ru.trykov.roskachestvo")

git_basic_authorization Base64.encode64(#{ENV['GIT_BASIC_AUTHORIZATION']})

